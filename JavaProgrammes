91

package BasicPrograms;

import java.util.Iterator;
import java.util.PriorityQueue;

public class pro90 {

	public static void display(PriorityQueue<Integer> pq)
	{
		Iterator<Integer> it=pq.iterator();
		System.out.println();
		while(it.hasNext())
		{
			System.out.print(it.next()+" ");
		}
	}
	
	public static void main(String[] args) {
		PriorityQueue<Integer> pq=new PriorityQueue<Integer>();
		
		pq.add(1);
		pq.add(2);
		pq.add(3);
		pq.add(4);
		pq.add(5);
		
		System.out.println(pq);
		
		System.out.println(pq.element());  // retrieves the head element from queue , If queue is empty 
		                                   // then it throws NoSuchElementException
		System.out.println(pq);        
		
		System.out.println(pq.peek());     // retrieves the head element from queue , If queue is empty then returns null
		display(pq);
		
		pq.poll();       // removes the element from queue
		display(pq);
		
		pq.offer(50);    // inserts an element in the queue
		display(pq);
		
		pq.remove(5);    // removes the specified element
		display(pq);
		
		pq.remove();     // removes head element
		display(pq);
	}

}

===========================================================================

92

package BasicPrograms;

import java.util.HashMap;
import java.util.Map;

public class pro91 {

	public static void main(String[] args) {
		HashMap<Integer, String> hm=new HashMap<Integer, String>();
		
		hm.put(101, "Atul");   // put() is adding key value pairs in the map
		hm.put(102, "Manoj");
		hm.put(104, "Sumit");
		hm.put(103, "Gaurav");
		hm.put(105, "Sonu");
		
		System.out.println(hm);

		hm.put(101, "Gaurav");
		
		System.out.println(hm);

		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
		
		System.out.println();
		hm.putIfAbsent(106, "Sanjay");   // If key not present in map then only it adds
		
		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
		
		hm.remove(105);   // removing the record on the basis of key
		
		System.out.println("After Removal : ");
		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
		
		System.out.println();
		hm.remove(102, "manoj"); // removing the record on the basis of key-value pair  
		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
		
		System.out.println("\nAfter replacing :");
		hm.replace(102, "Ravi");            // replacing the name at key=102
		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
		
		System.out.println();
		hm.replace(102, "Ravi", "Vikas");
		
		for(Map.Entry<Integer, String> m : hm.entrySet())
		{
			System.out.println(m.getKey()+" "+m.getValue());
		}
	}

}

==========================================================================

93
package BasicPrograms;

import java.util.HashMap;
import java.util.Map;

class Car
{
	String brand;
	int speed;
	long cost;
	public Car(String brand, int speed, long cost) {
		super();
		this.brand = brand;
		this.speed = speed;
		this.cost = cost;
	}
	@Override
	public String toString() {
		return "Car brand : " + brand + ", speed : " + speed + ", cost : " + cost ;
	}
}

public class pro92 {

	public static void display(HashMap<Integer, Car> hm)
	{
		for (Map.Entry<Integer, Car> m : hm.entrySet()) {
			int key=m.getKey();
			Car c=m.getValue();
			
			System.out.println("Record : "+key);
			System.out.println(c);
		}

	}
	
	public static void main(String[] args) {
		
		HashMap<Integer, Car> hm=new HashMap<Integer, Car>();
		
		// Object Creation
		Car c1=new Car("BMW", 250, 3000000);
		Car c2=new Car("Skoda",200, 2000000);
		Car c3=new Car("Audi", 150, 1500000);

		// Creating HashMap 
		hm.put(1, c1);      // Key = number(Integer) , value = object(car)
		hm.put(2, c2);
		hm.put(3, c3);
		
		
		display(hm);
		
		hm.put(4, new Car("Scorpio", 180, 1200000));
		
		System.out.println();
		System.out.println();
		display(hm);
		
		System.out.println();
		System.out.println("After removing record : ");
		hm.remove(2);
		display(hm);
	}

}

