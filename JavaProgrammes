package BasicPrograms;

import java.io.FileWriter;
import java.io.IOException;

public class pro100 {

	public static void main(String[] args) throws IOException {
		String str="This is a simple text";
		
		FileWriter ob1=new FileWriter("F://file5.txt"); // Character oriented class
		
		ob1.write(str);    // writes a complete string in characters
	
		ob1.write('D');  // writes a single character
		ob1.close();
	}

}
