95

package BasicPrograms;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.InputStream;

public class pro94 {

	public static void main(String[] args) throws Exception {
		
		// 1st way 
		FileInputStream fis=new FileInputStream("F://file1.txt");   // fis is input stream 
		
		fis.skip(6);      // Skips the no of bytes from the stream
		System.out.println("Bytes available : "+fis.available()); // Returns total bytes available in the file
		// 2nd way
		//File f=new File("F://file1.txt");
		//FileInputStream f1=new FileInputStream(f);
		
		int i=fis.read();    // reads data in the form of byte
		while(i!=-1)
		{
			System.out.print((char)i);
			
			i=fis.read();
		}
		
		System.out.println("\nBytes available : "+fis.available());
		fis.close();

		//System.out.println(fis.available()); // Invalid after closing the stream object
		
		fis=new FileInputStream("F://file1.txt");
		System.out.println(fis.available());
		
		byte buffer[]=new byte[fis.available()];
		
		fis.read(buffer);     // Reading data from file in the form of bytes in an array
		
		for(byte b:buffer)
		{
			 System.out.print((char)b);	   
		}
		
		fis.close();
	}

}

