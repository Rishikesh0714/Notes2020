		{
			z=10/no;
		} 
		catch (Exception e) 
		{
			z=10;
			System.err.print("Result on dividing number by 1, no passed is zero : ");
			System.out.println("Z : "+z);
		}
		
		System.out.println("Z : "+z);
	}
	
	
	public static void main(String[] args) {
		pro51 ob=new pro51();
		
