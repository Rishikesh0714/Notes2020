package BasicPrograms;

class baseclass
{
	public void display()
	{
		System.out.println("Hello");
	}
}

interface in11
{
	public void fun(int x);
}

public class pro117 extends baseclass implements in11{

	@Override
	public void display()
	{
		System.out.println("Welcome");
	}
	
	public static void main(String[] args) {
		pro117 p=new pro117();
		
		p.display();

	}

	@Override
	public void fun(int x) {
		// TODO Auto-generated method stub
		
	}

}

