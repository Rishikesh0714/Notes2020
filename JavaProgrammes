89

package BasicPrograms;

import java.util.Iterator;
import java.util.TreeSet;

public class pro88 {

	public static void main(String[] args) {
		TreeSet<Integer> ts=new TreeSet<Integer>();
		
		ts.add(500);
		ts.add(900);
		ts.add(700);
		ts.add(300);
		ts.add(100);
		ts.add(200);
		
		System.out.println(ts);

		Iterator<Integer> it=ts.descendingIterator();
		
		while(it.hasNext())
		{
			System.out.print(it.next()+" ");
		}
		
		try 
		{
			ts.add(null);
		} 
		catch (Exception e) {
			System.err.println("\nNull can not be added in Set");
		}
		System.out.println();
		System.out.println(ts);
		
		ts.pollFirst();  // removing minimum element
		System.out.println(ts);
		
		ts.pollLast();   // removing maximum element
		System.out.println(ts);
		
		System.out.println(ts.descendingSet());
		
		System.out.println("Highest element : "+ts.last());
		System.out.println("Lowest element : "+ts.first());
		System.out.println(ts.ceiling(301));  // returns the least element greater than or equal to given element
		System.out.println(ts.floor(600));    // returns the greatest element which is less than or equal to the given element
		
		System.out.println("Set size : "+ts.size());
		
	}

}

