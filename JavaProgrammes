package BasicPrograms;

import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;

public class pro95 {

	public static void main(String[] args) throws IOException {
		FileOutputStream fos=new FileOutputStream("F://file4.txt",true);  // The file is opened in append mode
		
		fos.write('E');  // Writes a single byte to the file
		fos.close();
		
		fos=new FileOutputStream("F://file2.txt");
		
		String str="This text is written in the file";
		byte buffer[]=str.getBytes();
		
		fos.write(buffer);   // writes collection of bytes in the stream
		
		fos.close();
	}

}

