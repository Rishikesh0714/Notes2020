package BasicPrograms;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.SQLException;
import java.sql.Statement;
import java.util.Scanner;

public class pro106 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		String name=null;
		int password=0;
		
		try {
			 Class.forName("oracle.jdbc.driver.OracleDriver");  // Load & register driver
			 
			 Connection cn = DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe","system","root"); // Establishes a connection
		
			 Statement smt = cn.createStatement();  // Create a statement to send the query to the database
		
			 System.out.println("Enter username : ");
			 name=sc.nextLine();
			 
			 System.out.println("Enter Password : ");
			 password=sc.nextInt();
			 
			 String query="insert into loginDetails values('"+name+"',"+password+")";
			
			 int i=smt.executeUpdate(query);  // Execute a query
			 
			 System.out.println(i+" Record added successfully....");
			 smt.close();
			 
			 cn.commit();
			 cn.close();   // close connection
			
		} catch (ClassNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
