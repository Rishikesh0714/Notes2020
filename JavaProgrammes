package BasicPrograms;

public class pro49 {

	
	public static void main(String[] args) {
		int x=20,z=0;
		
		try
		{
			z=x/2;     // Raising the exception
			System.out.println("Z : "+z);
		}
		catch(Exception e)
		{
			System.out.println("Can not divide by zero");
		}
		
		System.out.println(z);
		System.out.println("End of program");

	}

}

