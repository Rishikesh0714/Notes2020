100

package BasicPrograms;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.io.ObjectOutputStream;

public class pro98 {

	public static void main(String[] args) throws IOException, ClassNotFoundException {
		int num1=50;
		String str="Hello";
		
		FileOutputStream fo=new FileOutputStream("F://objectfile.txt");
		ObjectOutputStream ob1=new ObjectOutputStream(fo);
		
		ob1.writeInt(num1);
		ob1.writeObject(str);
		
		FileInputStream fi=new FileInputStream("F://objectfile.txt");
		ObjectInputStream ob2=new ObjectInputStream(fi);
		
		System.out.println("num1 : "+ob2.readInt());
		System.out.println("str  : "+ob2.readObject());
		
		ob1.close();
	}

}

====================================================================

101

package BasicPrograms;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.io.FileOutputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.io.ObjectOutputStream;
import java.io.Serializable;

class Players implements Serializable   // Class Player will become serializable
{
	String name;
	String city;
	int score;
	public Players(String name, String city, int score) {
		this.name = name;
		this.city = city;
		this.score = score;
	}
}

public class pro99 {

	public static void main(String[] args) throws IOException, ClassNotFoundException {
		FileOutputStream fos=new FileOutputStream("F://players.ser");
		ObjectOutputStream ob1=new ObjectOutputStream(fos);
		
		Players p1=new Players("Sachin", "Mumbai", 20000);
		
		ob1.writeObject(p1);   // Object p1 is written into the file players.ser  (serializaion)
		
		FileInputStream fis=new FileInputStream("F://players.ser");
		ObjectInputStream ob2=new ObjectInputStream(fis);
		
		Players p = (Players) ob2.readObject();       // (deserialization)
		
		System.out.println("Name : "+p.name);
		System.out.println("City : "+p.city);
		System.out.println("Score : "+p.score);
		
		ob2.close();
		ob1.close();
		fis.close();
		fos.close();
	}
}

