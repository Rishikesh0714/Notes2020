4

package org.servlets;

import java.io.IOException;
import java.io.PrintWriter;
import java.util.Enumeration;

import javax.servlet.ServletConfig;
import javax.servlet.ServletContext;
import javax.servlet.ServletException;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

/**
 * Servlet implementation class ser1
 */
public class ser1 extends HttpServlet {
	private static final long serialVersionUID = 1L;
       
    /**
     * @see HttpServlet#HttpServlet()
     */
    public ser1() {
        super();
        // TODO Auto-generated constructor stub
    }

	/**
	 * @see HttpServlet#doGet(HttpServletRequest request, HttpServletResponse response)
	 */
	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		PrintWriter out=response.getWriter();
		
		ServletConfig sc=getServletConfig();
	
		String prname=sc.getInitParameter("productName");
		
		out.print("Product Name : "+prname);
		
		String qty=sc.getInitParameter("Quantity");
		
		out.print("<br>Quantity : "+qty);
		
		String ct=sc.getInitParameter("CustType");
		out.print("<br>Customer type  : "+ct);
		
		out.print("<br>===================================================<br>");
		
		Enumeration<String> e=sc.getInitParameterNames();
		
		String st="";
		while(e.hasMoreElements())
		{
			st=e.nextElement();    // nextElement() returns parameter name
			out.println(st+" ");
			out.print(sc.getInitParameter(st));   // getInitParameter() returns the value on the parameter name
		}
		
		out.print("<br>Servlet Name : "+getServletName());
		
		
		ServletContext context=getServletContext();
		
		out.print("<br>City : "+context.getInitParameter("City"));
	
		out.print("<br>==================================================<br>");
		
		Enumeration<String> e1=context.getInitParameterNames();
	
		while(e1.hasMoreElements())
		{
			st=e1.nextElement();    // nextElement() returns parameter name
			out.println(st+" ");
			out.print(context.getInitParameter(st));   // getInitParameter() returns the value on the parameter name
		}
	}

	/**
	 * @see HttpServlet#doPost(HttpServletRequest request, HttpServletResponse response)
	 */
	protected void doPost(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {
		// TODO Auto-generated method stub
		doGet(request, response);
	}

}
