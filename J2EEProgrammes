<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<form action="SetUserDetails.jsp" method="get">
Name<input type="text" name="username"><br>
City<input type="text" name="city"><br>
<input type="submit" value="Submit">
</form>
</body>
</html>

<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<jsp:useBean id="u" class="org.pro.Users" scope="session"></jsp:useBean>

<%--
<jsp:setProperty property="username" name="obj"/>
<jsp:setProperty property="city" name="obj"/>
 --%>
 
 <jsp:setProperty property="*" name="u"/>   <!-- Setting all properties of Bean Class -->
 
<br>
<h3>User Details</h3>
Name :&nbsp;&nbsp; <jsp:getProperty property="username" name="u"/><br>
City :&nbsp;&nbsp; <jsp:getProperty property="city" name="u"/>
</body>
</html>


