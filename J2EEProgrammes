  </servlet>
  <servlet-mapping>
    <servlet-name>SetterServlet</servlet-name>
    <url-pattern>/SetterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetterServlet</display-name>
    <servlet-name>GetterServlet</servlet-name>
    <servlet-class>org.servlets.GetterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetterServlet</servlet-name>
    <url-pattern>/GetterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ser10</display-name>
    <servlet-name>ser10</servlet-name>
    <servlet-class>org.servlets.ser10</servlet-class>
  </servlet>
  <servlet-mapping>
