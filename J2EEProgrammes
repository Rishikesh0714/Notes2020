 <!-- Action Tag ( Standard Tag)  -->
 <jsp:useBean id="obj" class="org.pro.Users" scope="session"></jsp:useBean>
 <br>
 Name : <jsp:getProperty property="username" name="obj"/><br>  <!-- Call getter method of bean class on bean object obj -->
 City : <jsp:getProperty property="city" name="obj"/>
 <br>
<br>
<h5>After updating Bean values</h5>
<jsp:setProperty property="username" name="obj" value="Sachin"/>   <!-- Call setter method of bean class on bean object obj -->
<jsp:setProperty property="city" name="obj" value="Chennai"/>

 <br>
 Name : <jsp:getProperty property="username" name="obj"/><br>
 City : <jsp:getProperty property="city" name="obj"/>


</body>
</html>

