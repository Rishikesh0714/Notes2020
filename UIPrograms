<html>
    <head>
    </head>
    
    <body>  
        
        <button onclick="getSum(2,3)">Get Sum</button>
        <script>
           
            // Function not returning any value / function declartion
            function getMessage()
            {
                document.write("Message from function with no parameters");    
            }
            
            function getNumber(num)
            {
                document.write("Message from function with parameter "+num);
            }
            
            function getNum(num1,num2)
            {
                document.write("Message from function with parameter "+(num1+num2));
            }
            
            
            // Function returning value
            function getSum(num1,num2)
            {
                document.write("Sum : "+(num1+num2));
                //return (num1+num2);
            }
            
            
            // Function Expression
            
            var myfunction=function()
            {
                document.write("Function expression with no parameters");
            }
            
            var display=function(n1,n2)
            {
                document.write("Function expression with parameters : <b>"+n1+" and "+n2+"</b>");
            }
           
            //  f();  // Not possible , Throws error
            
            var f=function()
            {
                document.write("Hello");
            }
            
            
             getMessage();
             document.write("<br>");
             getNumber(30,20);
             document.write("<br>");
             getNum(30,20);
             document.write("<br>");
             var res= getSum(4,5);
             document.write("Sum : "+res);
             document.write("<br>");
             document.write("Sum : "+getSum(5,6));
             document.write("<br>");
             document.write("Sum : "+getNum(3,5)); 
             document.write("<br>");
             myfunction();
             document.write("<br>");
             display(30,40);
            
        </script>
    </body>
</html>

