<html>
    <head>
        <style>
          #i1
            {
                color: brown;
            }
            
           #i2
            {
                color: blue;
            }
        </style>
    </head>
    <body>
      
          Name : <h1 id="i1"></h1>
        Age  :  <h1 id="i2"></h1>
        <form name="myform">
            Name : <input type="text" name="nm" onfocus="f1(this)" onblur="f2(this)"><br>
            Age  : <input type="text" name="ag" onfocus="f1(this)" onblur="f2(this)"><br>
            <input type="button" onclick="fun()" value="Submit">
        </form>
      
        
        <script>
          function fun()
            {
               var name= document.myform.nm.value;  // Assigning textbox value in a variable name
               var age= document.myform.ag.value;   // Assigning textbox value in a variable age
               
               document.getElementById("i1").innerHTML=name;
               document.getElementById("i2").innerHTML=age;
               //document.getElementById("i1").style.color="RED";
               //document.getElementById("i2").style.color="Blue";
            }
            
            function f1(x)
            {
                x.style.backgroundColor="cyan";
                x.style.color="Brown";
            }
            
            function f2(x)
            {
                x.style.backgroundColor="white";
                x.style.color="Brown";
            }
        </script>
    </body>
</html>

