<html>
    <head>
        <style>
        div
            {
                width: 150px;
                height: 150px;
                background-color: blue;
            }
        </style>
      <!-- <script src="jquery-3.5.1.min.js"></script> -->
         <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
         <script>
            $(document).ready(function(){ 
                
                $(".c1,.c2").click(function(){   // Applying multiple classes in a selector
                  $(this).css("color","blue"); 
               });
                
               $("#i1,#i2").click(function(){
                  $(this).css("color","red"); 
               });
               
              $("div").click(function(){
                  $(this).css("backgroundColor","yellow"); 
               });
               
               $("div").hover(function(){
                  $(this).css("backgroundColor","brown"); 
                   alert("hover");
               });    
            
               $("h1,h2").click(function(){     // Applying multiple elements in a selector
                  $(this).css("backgroundColor","green"); 
               });
            
                $("#i3").dblclick(function(){
                   $(this).hide(); 
                });
                $("#i4").mouseenter(function(){
                   $(this).css("backgroundColor","red"); 
                });
                $("#i4").mouseleave(function(){
                   $(this).css("backgroundColor","blue"); 
                });
                 $("#i4").dblclick(function(){
                   $(this).css("backgroundColor","yellow"); 
                });
                 $("#i4").click(function(){
                   $(this).css("backgroundColor","black"); 
                });
                
                
                
            }); 
        </script>
    </head>
    
    <body> 
        <p class="c1">Para1</p>
        <p class="c2">Para1</p>
        <p id="i1">Para3</p>
        <p id="i2">Para4</p>
        <div></div>
        <h1>H1</h1>
        <h2>H2</h2>
        <p id="i3">Double click me</p>
        <div id="i4"></div>
    </body>
</html>

