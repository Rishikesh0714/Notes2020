         Enter Password : <input type="password" name="pw" onkeyup="fun()">
        </form>
        
        <b>Strength : <span id="i1">No Strength</span></b>
        <script>
            function fun()
            {
                var msg;
                if(document.myform.pw.value.length > 4)
                    {
                        msg="Good";
                    }
                else{
                    msg="Poor";
                }
                document.getElementById("i1").innerText=msg;
            }
         </script>
    </body>
