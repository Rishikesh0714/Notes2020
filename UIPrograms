       <script> 
             var city=new Array("Pune","Mumbai","Chennai","Delhi");
             
             document.write(city.join());  // Joining elements with ,
             document.write("<br>");
             document.write(city.join("->"));  // Joining elements with ->
              document.write("<br>");
             var str=city.join("*");         // Joining elements with * and assigns complete string in str
             document.write(str);
             document.write("<br>");
             document.write(city.toString());  // Returns comma seperated string with array elements
             var subarray=city.slice(0,2);  // returns subarray from index 0 to 1
             document.write("<br>");
             document.write(subarray);
           
             var subarray=city.slice(1);  // returns subarray from index 1 onwards
             document.write("<br>");
             document.write(subarray);
           
             var subarray=city.slice(-2);  // returns subarray from the end upto 2 elements
             document.write("<br>");
             document.write(subarray);
           
             document.write(subarray[0]); // displaying first element of subarray
             document.write("<br>");  
           
             var n1=[1,2,3,4,5];
             var n2=[10,20,30,40,50];
             var n3=["a","b","c"];  
           
            var mergeArray=n1.concat(n2);  // Concatenates two or more arrays
            document.write(mergeArray);
            
             document.write("<br>"); 
             var m1=n3.concat(n1,n2);
             document.write(m1);
           
             document.write("<br>"); 
             document.write(city.reverse());   // reverses the array elements
         
             var name="Sachin";
           
            document.getElementById('id1').innerHTML=name;
        </script>
    </body>
</html>

