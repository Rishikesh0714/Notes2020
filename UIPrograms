<html>
    <head>
       
    </head>
    <body>
        <form name="myform" action="page43.html" onsubmit="return validateInfo()">
            Username       : <input type="text" name="un" required><br>
            Enter Password : <input type="password" name="pw" required><br>
            <input type="submit" value="Login"><input type="button" onclick="signup()" value="Sign up">
        </form>
        
         <script>
            function validateInfo()
            {
                var user=document.myform.un.value;
                var pass=document.myform.pw.value;
                
                if(user=="Ashutosh" && pass=="1234")
                    {
                        alert("Logged in successfully");
                        return true;
                    }
                else
                    {
                        alert("Please signup");
                        return false;
                    }
            }
             
            function signup()
             {
                window.location="page40.html";    // used to get information about current page and redirect control to other page
             }
             
         </script>
    </body>
</html>

