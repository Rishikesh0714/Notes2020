29

<html>
    <head>
    
    </head>
    
    <body>  
        <script>
            var a=10,b=5;
            
            // Arithmatic operators
            /*
            alert("Addition : "+(a+b));
            alert("Subtraction : "+(a-b));
            alert("Multiplication : "+a*b);
            alert("Division : "+a/b);
            alert("Remainder : "+(a%b));   
            */
            // Assignment operators   -=, *=,/=, %=
            a+=5;     // a=a+5
            console.log("a : "+a);
            
            //String operators  +(concatenation) , += (concatenation assignment)
            var s1="Hello";
            var s2="World";
            
            document.write(s1+" "+s2);
            
            s1+="Ashutosh";
            document.write("<br>Message  : "+s1+"<br>");
            
            //Increment operator(++p : preincrement),(p++ : postincrement)
            var i=50;
            
            document.write("i : "+i++);
            document.write("<br> i : "+i);    // i=51
            
            document.write(++i);              // i=52
            
            //Logical operators ( && (AND), || (OR), !(NOT))
            
            document.write("<br>Result : "+((10>5)&&(5<2)));
            document.write("<br>Result : "+((10>5)||(5<2)));
            document.write("<br>Result : "+!((10>5)&&(5<2)));  // !true = false , !false = true
            
            // Comparision Operators (==,<,<=,>=,>!=)
            
            var i1=10;
            var i2=20
            var i3="10";
            
            document.write("<br>");
            
            document.write("i1==i2 : "+(i1==i2));    //false
            
            document.write("<br>");
            
            document.write("i1==i3 : "+(i1==i3));   // == only checks the value not data type
            
            document.write("<br>");
            document.write("i1===i3 : "+(i1===i3));  // === only checks the value and data type
           
            document.write("<br>");
            document.write("i1!=i3 : "+(i1!=i3));
            
            document.write("<br>");
            document.write("i1!==i3 : "+(i1!==i3));  // !== Not identical operator
            
            document.write("i1>i2 : "+(i1>i2));  

            document.write("<br>");
           
            // Concept of let , var and const
            
            let name="Sachin";
            document.write("Name : "+name);
            
            document.write("<br>");
            name="Ajay";
            document.write("Name : "+name);
            
            document.write("<br>");
            
            const PI=3.14;
            document.write("PI : "+PI);
            
            // PI=2.2;  // Can not be reassigned
            
            // ES5 (ECMA script)
            
            var age=20;
            var age=30;
            
            document.write("Age : "+age);
            
            // ES6 (ECMA script)

            let ag=20;
            let ag1=30;
            
            document.write("Age : "+ag);
            document.write("<br>");
            
            // Block { }
            {
                var v=20;
                document.write("v : "+v);
            }
            document.write("<br>");
            document.write("v : "+v);
            
            
            document.write("<br>");
            //let v1=50;
            {
                let v1=40;
                document.write("v1 : "+v1);
            }
            document.write("<br>");
            //document.write("v1 : "+v1);
            
            var _val=300;
            document.write(_val);
            
        </script>
    </body>
</html>

