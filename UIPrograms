57

<html>
    <head>
        <style>
        div
            {
                width: 150px;
                height: 150px;
                background-color: blue;
            }
           
        </style>
      <!-- <script src="jquery-3.5.1.min.js"></script> -->
         <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
         <script>
            $(document).ready(function(){ 
                
                $("input.c1").focus(function(){
                    $(this).css("backgroundColor","yellow");
                });
                 $("input.c1").blur(function(){
                    $(this).css("backgroundColor","white");
                });
               
                /*
                $("p").on("dblclick",function(){   // Applying click & hide event on a single paragraph
                   $(this).hide(); 
                });
                */
               
                $("div").on({ mouseenter:function(){
                    $(this).css("backgroundColor","red");    
                },
                            mouseleave:function(){
                                 $(this).css("backgroundColor","yellow");
                            } });
                $("input").change(function(){   // Textfield & select box event
                   alert("Text has been changed"); 
                });
               
                  $("form").submit(function(){
                      message();
                
                  });
                
                $("input#in1").keypress(function(){   // When 
                     $(this).css({"color":"brown","font-size":"20px"});
                });
                
                $("input#in2").keydown(function(){   // When 
                     $(this).css({"backgroundColor":"cyan","font-size":"20px"});
                });
                
                 $("input#in2").keyup(function(){   // When 
                     $(this).css({"backgroundColor":"red","font-size":"20px"});
                });
                
                $("form :submit").css("backgroundColor","pink");
             
            }); 
        </script>
    </head>
    
    <body> 
        <hr>
        
        <p>This is a paragraph</p>
        <div></div>
        
        <form action="">
        Name<input type="text" class="c1"><br>
        Age<input type="text" class="c1"> 
        City<input type="text" id="in1">
        Email<input type="text" id="in2">
        <input type="reset" value="Clear">
            <input type="submit" value="Submit">
        </form>
        
        
        <button>Click me</button>
