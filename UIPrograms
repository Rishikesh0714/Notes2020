<html>
    <head>
       
    </head>
    <body>
            <input type="button" onclick="fun()" value="Open Form">
            <div id="i1"></div> 
            <h3 id="i2"></h3>
        <script>
            
        var temp=true;    
          function fun()
            {
               
                var entryForm="<form action=page43.html> Enter Name <input type=text name='nm'> <br> <input type=submit value='Go'></form>";
                
                if(temp)
                    {
                        
                         document.getElementById("i1").innerHTML=entryForm;
                         temp=false;    
                       
                    }
                else
                    {
                         document.getElementById("i1").innerHTML="";
                         temp=true; 
                         
                    }
               
                 document.getElementById("i2").innerHTML=temp;
            }
            </script>
    </body>
</html>

