<html>
    <head>
    </head>
    
    <body>  
       <script> 
           
             function getNoOfCities(cy)     // Passing array to function
             {
                 document.write("Total Cities : "+cy.length);
             }
           
             
           
            var colors=["red","blue","green"];   // Declaration
           
             document.write(colors[0]);    // Accessing Array elements
             document.write(colors[1]);
             document.write(colors[2]);

             var city=new Array("Pune","Mumbai","Chennai");
             document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
             document.write("</ol>");
           
            getNoOfCities(city);
           
            document.write("<br>");
            city.push("Delhi");    // Push() , adding element at the end of array
           
            getNoOfCities(city);
          
             document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
             document.write("<li>"+city[3]+"</li>");
             document.write("</ol>");
           
           city.unshift("Hyderabad");  // Adding element in begining
           
           document.write("<li>"+city[0]+"</li>");    // Accessing Array elements

           document.write("Last element : "+city[4]); 
           
           city.pop();                            // Remove last element
            
           document.write("Last element : "+city[4]); 
           
           city.shift();                         // Remove first element
           
           document.write("First element : "+city[0]); 

            document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
             
             document.write("</ol>");
           
             city.splice(1,1);   // Removes element at first index
           
              document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
            
             document.write("</ol>");
            
               city.splice(1,0,"Kanpur");   // Adding element at first index
           
            
              document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
             document.write("</ol>");
            
              city.splice(1,0,"Jaipur","Indore");        // Adding 2 elements at first index
           
            
              document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
              document.write("<li>"+city[3]+"</li>");
            document.write("<li>"+city[4]+"</li>");
             document.write("</ol>");
           
            
              city.splice(1,2);                    // Removes 2 elements at first index
           
            
              document.write("<ol>");
             document.write("<li>"+city[0]+"</li>");    // Accessing Array elements
             document.write("<li>"+city[1]+"</li>");
             document.write("<li>"+city[2]+"</li>");
        
             document.write("</ol>");
            
           
        </script>
    </body>
</html>

